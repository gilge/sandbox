pipeline {
    agent any

    stages {
        stage ('War Compile Stage') {

            steps {
                withMaven(maven : 'maven_3_5_0') {
                    sh 'mvn -f war-sample/pom.xml  clean compile'
                }
            }
		}
		stage ('War Deployment Stage') {

            steps {
                withMaven(maven : 'maven_3_5_0') {
                    sh 'mvn -f war-sample/pom.xml  deploy'
                }
            }
		}
	}
}